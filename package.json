{
  "name": "dynamic-form-blundert",
  "version": "0.0.1",
  "description": "Dynamic form from configuration with React",
  "author": "Matteo Granzotto",
  "license": "MIT",
  "repository": "Blundert/dynamic-form",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "deploy": "npm run deploy:documentation",
    "deploy:documentation": "firebase use default && firebase deploy",
    "predeploy": "npm run build:storybook",
    "prepublish": "run-s build",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "storybook": "npm run prebuild:storybook && start-storybook",
    "test": "run-s test:unit test:lint test:build",
    "test:jest": "jest",
    "prebuild:storybook": "npm run test:generate-output",
    "build:storybook": "build-storybook -c .storybook -o build/",
    "test:build": "run-s build",
    "test:generate-output": "jest --json --outputFile=./src/.jest-test-results.json || true",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom"
  },
  "peerDependencies": {
    "react": ">=16.0.1",
    "react-dom": ">=16.0.1",
    "react-jss": ">=10.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-jest": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-notes": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-create-react-app": "^2.1.1",
    "@storybook/react": "^5.3.18",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^25.3.0",
    "microbundle-crl": "^0.13.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-jss": ">=10.1.1",
    "react-scripts": "^3.4.1",
    "react-test-renderer": "^16.13.1"
  },
  "files": [
    "dist"
  ]
}
